      },
      "config": {
        "googleMapsApiKey": "AIzaSyDwCmFr8Mfs2JTWjNxjMI1n6R1sv_-aBOo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
